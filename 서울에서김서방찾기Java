class Solution {
    public String solution(String[] seoul) {
        String answer = "";
        //배열을 for반복문으로 돌려서 kim과 일치하는 배열을 찾으면 answer 반환
        for (int i = 0; i < seoul.length; i++){
            if (seoul[i].equals("Kim")){
                answer = "김서방은 " + i + "에 있다";
                break;
            }
        }
        return answer;
    }
}