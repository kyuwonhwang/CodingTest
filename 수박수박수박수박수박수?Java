class Solution {
    public String solution(int n) {
        String answer = "";
        
        for(int i=1; i<=n; i++) {
        //입력받은 숫자를 2로 나눠서 나머지가 0이면 짝수이므로 "수"를 answer에 입력
        if(i%2 != 0) answer += "수";
        //나머지가 0이 아니면 홀수이므로 "박"을 입력.
        else answer += "박";
    }
    return answer;
}
